#include<ncurses.h>
#include<stdlib.h>
#include"start.h"

int main() {
	
	int x, y, i, t;
	initscr();
	start_color();
	init_pair(1, COLOR_CYAN, COLOR_YELLOW);
	init_pair(2, COLOR_MAGENTA, COLOR_MAGENTA);
	init_pair(3, COLOR_WHITE, COLOR_CYAN);
	init_pair(4, COLOR_BLACK, COLOR_CYAN);
	init_pair(5, COLOR_RED, COLOR_BLUE);
	init_pair(6, COLOR_RED, COLOR_YELLOW);
	init_pair(7, COLOR_BLACK, COLOR_CYAN);
	
	call();
	clear();
	
	
	getch();
	endwin();
	
	return 0;
}	
